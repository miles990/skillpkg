{
  "name": "skillpkg-cli",
  "version": "0.5.5",
  "description": "CLI for skillpkg - Agent Skills Package Manager. Install once, use everywhere.",
  "type": "module",
  "bin": {
    "skillpkg": "./dist/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js"
  },
  "keywords": [
    "cli",
    "skillpkg",
    "agent",
    "skills",
    "ai",
    "claude",
    "copilot",
    "codex",
    "llm",
    "package-manager"
  ],
  "author": "miles990",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miles990/skillpkg.git",
    "directory": "packages/cli"
  },
  "homepage": "https://github.com/miles990/skillpkg#readme",
  "bugs": {
    "url": "https://github.com/miles990/skillpkg/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.3",
    "commander": "^12.0.0",
    "dotenv": "^17.2.3",
    "gray-matter": "^4.0.3",
    "inquirer": "^9.2.0",
    "ora": "^8.0.0",
    "skillpkg-core": "^0.5.3",
    "tar-stream": "^3.1.7"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.0",
    "@types/node": "^20.10.0",
    "@types/tar-stream": "^3.1.4",
    "typescript": "^5.3.0"
  }
}
